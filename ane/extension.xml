<extension xmlns="http://ns.adobe.com/air/extension/50.0">
  <id>com.vsdevelop.air.extension.glfw</id>
  <versionNumber>1.0</versionNumber>
  <platforms>
    <platform name="Windows-x86">
        <applicationDeployment>
            <nativeLibrary>ANEGLFW.dll</nativeLibrary> 
            <initializer>ANEGLFWExtInitializer</initializer> 
            <finalizer>ANEGLFWExtFinalizer</finalizer> 
        </applicationDeployment>
    </platform>
    <platform name="Windows-x86-64">
        <applicationDeployment>
            <nativeLibrary>ANEGLFW.dll</nativeLibrary> 
            <initializer>ANEGLFWExtInitializer</initializer> 
            <finalizer>ANEGLFWExtFinalizer</finalizer> 
        </applicationDeployment>
    </platform>
    <platform name="default">
